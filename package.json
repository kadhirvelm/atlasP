{
  "name": "atlasp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^2.1.1",
    "@blueprintjs/docs-theme": "^2.1.1",
    "axios": "^0.18.0",
    "googleapis": "^32.0.0",
    "husky": "^0.14.3",
    "lodash": "^4.17.5",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.1",
    "readline": "^1.3.0",
    "redoodle": "^2.3.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "twilio": "^3.17.4",
    "underscore": "^1.8.3"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "lint": "tslint src/**/*.ts && tslint src/**/*.tsx",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.1.0",
    "@types/lodash": "^4.14.107",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.3.3",
    "@types/react": "^16.3.11",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^6.0.2",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-transition-group": "^2.0.8",
    "@types/underscore": "^1.8.8",
    "eslint-plugin-prettier": "^2.6.1",
    "prettier": "^1.13.6",
    "pretty-quick": "^1.6.0",
    "react-scripts-ts": "2.16.0",
    "typescript": "^2.8.1"
  },
  "collectCoverageFrom": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "!src/**/*.test.ts",
    "!src/**/*.test.tsx",
    "!src/index.tsx",
    "!src/registerServiceWorker.ts"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80,
      "statements": 100
    }
  }
}
